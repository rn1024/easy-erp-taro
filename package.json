{
  "name": "easy-erp-taro",
  "version": "1.0.0",
  "private": true,
  "description": "Easy ERP Taro小程序版本",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:jd": "taro build --type jd",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:jd": "npm run build:jd -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.24.0",
    "@nutui/nutui-react-taro": "^3.0.16",
    "@tarojs/components": "4.1.2",
    "@tarojs/helper": "4.1.2",
    "@tarojs/plugin-framework-react": "4.1.2",
    "@tarojs/plugin-platform-alipay": "4.1.2",
    "@tarojs/plugin-platform-h5": "4.1.2",
    "@tarojs/plugin-platform-jd": "4.1.2",
    "@tarojs/plugin-platform-qq": "4.1.2",
    "@tarojs/plugin-platform-swan": "4.1.2",
    "@tarojs/plugin-platform-tt": "4.1.2",
    "@tarojs/plugin-platform-weapp": "4.1.2",
    "@tarojs/react": "4.1.2",
    "@tarojs/runtime": "4.1.2",
    "@tarojs/shared": "4.1.2",
    "@tarojs/taro": "4.1.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "taro-icons": "^0.4.0",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.0",
    "@tarojs/cli": "4.1.2",
    "@tarojs/webpack5-runner": "4.1.2",
    "@types/react": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "babel-preset-taro": "4.1.2",
    "eslint": "^8.12.0",
    "eslint-config-taro": "4.1.2",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "postcss": "^8.4.18",
    "react-refresh": "^0.14.0",
    "sass": "^1.56.0",
    "typescript": "^4.7.0",
    "webpack": "5.69.0"
  }
